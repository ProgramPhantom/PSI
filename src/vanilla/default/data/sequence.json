{
    "x": 0,
    "y": 0,

    "padding": [0, 4, 0, 4],
    "offset": [0, 0],

    "grid": {
        "gridOn": true,
        "gridPositioning": "start",
        "line": {
            "style": {
                "stroke": "black",
                "strokeWidth": 1,
                "dashing": [5, 7]
            }
        }
    },

    "bracket": {
        "orientation": "vertical",
        "protrusion": 2,
        "adjustment": [0, 3],

        "direction": "left",

        "style": {
            "strokeWidth": 1,
            "bracketType": "square",
            "expression": 0.5,
            "stroke": "black"
        },

        "labelOn": true,
        "label": {
            "text": "\\tau",
            "padding": [2, 2, 3, 2],
            "position": "bottom",
            "style": {
                "size": 10,
                "colour": "black"
            }
        }
    }
    
}